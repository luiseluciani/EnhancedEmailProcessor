    <fields>
        <fullName>Can_Process__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
NOT ISPICKVAL(Status__c, &apos;Sent&apos;) 
&amp;&amp; NOT ISPICKVAL(Status__c, &apos;Error&apos;) 
&amp;&amp; NOT (ISPICKVAL(Status__c, &apos;InProgress&apos;) &amp;&amp; Status_Date_Time__c&gt; NOW()- (6/1440))
&amp;&amp; (No_Of_Tries__c=null || No_Of_Tries__c &lt; 3), 
true, 
false 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Can Process</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
